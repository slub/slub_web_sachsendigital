        tt_content_defValues {
          CType = image_ldp_curated
        }
      }
      menu_collections {
        iconIdentifier = content-menu-pages
        title = LLL:EXT:slub_web_sachsendigital/Resources/Private/Language/locallang.xlf:tx_slubwebsachsendigital_media.menu_collections
