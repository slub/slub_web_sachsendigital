        description = LLL:EXT:slub_web_sachsendigital/Resources/Private/Language/locallang.xlf:tx_slubwebsachsendigital_media.media.description
        tt_content_defValues {
          CType = list
          list_type = slubwebsachsendigital_media
        }
      }
      image_ldp_institutions {
        iconIdentifier = content-image
